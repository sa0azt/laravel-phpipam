{
    "name": "axsor/laravel-phpipam",
    "description": "PhpIPAM wrapper for laravel",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "axsor",
            "email": "alexsorribes@e-ports.eu"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Axsor\\PhpIPAM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Axsor\\PhpIPAM\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axsor\\LaravelPhpIPAM\\PhpIPAMServiceProvider"
            ],
            "aliases": {
                "PhpIPAM": "Axsor\\LaravelPhpIPAM\\PhpIPAMFacade"
            }
        }
    }
}
